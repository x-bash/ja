
___x_cmd_ja_tokenize(){
    command awk -v ORS="${1:-\n}" \
                -f "$___X_CMD_ROOT_MOD/awk/lib/json.awk" \
                -f "$___X_CMD_ROOT_MOD/ja/lib/awk/tokenized_main.awk"
}

___x_cmd_ja_fmt(){
    command awk -v INDENT="${INDENT:-2}" -f "$___X_CMD_ROOT_MOD/jo/lib/fmt.awk"
}

___x_cmd_ja_color(){
    command awk -v INDENT="${INDENT:-2}" -f "$___X_CMD_ROOT_MOD/jo/lib/color.awk"
}

