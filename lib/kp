
___x_cmd_jawk_kp(){
    local IFS=' '
    ___x_cmd_jawk_tokenized | \
    command awk -f "$__X_CMD_ROOT_MOD/awk/lib/json.awk" \
                -f "$__X_CMD_ROOT_MOD/awk/lib/jiter.awk" \
                -f "$__X_CMD_ROOT_MOD/jawk/lib/kp.awk" | \
    command awk '
{
    a = 1 - a
    if (a == 0) val = $0
    else {
        kp = $0
        kal = split(kp, ka, "\001")
        k = ka[kal]
        next()
    }
}
'"
$*
"

}

